local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "GUNS RNG ", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})
local Tab = Window:MakeTab({
	Name = "POTIONS",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = Tab:AddSection({
	Name = "INF POTION"
})
Tab:AddButton({
    Name = "SuperRollSpeedPotion ",
    Callback = function()
        local ohString1 = "SuperRollSpeedPotion"--can change to other potions 
local ohNumber2 = math.huge

game:GetService("ReplicatedStorage").Packages.Knit.Services.PlayerBoostService.RF.AddPlayerBoostUseCount:InvokeServer(ohString1, ohNumber2)
    end
})

